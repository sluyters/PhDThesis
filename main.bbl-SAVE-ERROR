    \entry{Gigie:2019}{inproceedings}{}
      \name{author}{5}{}{%
        {{hash=32146a30aeea8f2e7439946227b68ccd}{%
           family={Gigie},
           familyi={G\bibinitperiod},
           given={Andrew},
