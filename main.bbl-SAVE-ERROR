    \entry{Hazra:2019b}{inproceedings}{}
      \name{author}{2}{}{%
        {{hash=1f8393e7b55c6e636efc0f5f02199812}{%
           family={Hazra},
           familyi={H\bibinitperiod},
           given={Souvik},
