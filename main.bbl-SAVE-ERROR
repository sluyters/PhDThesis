    \entry{Kim:2017a}{inproceedings}{}
      \name{author}{2}{}{%
        {{hash=87cdc5e1c98297b2d39a303238324992}{%
           family={Kim},
           familyi={K\bibinitperiod},
           given={Youngwook},
           giveni={Y\bibinitperiod}}}%
        {{hash=ca18bab772baab4b039410615bdd3843}{%
           family={Toomajian},
           familyi={T\bibinitperiod},
           given={Brian},
           giveni={B\bibinitperiod}}}%
