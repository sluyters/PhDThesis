      \strng{fullhash}{cd0676ac5790a1851ce9d5b8c95f0a52}
      \strng{bibnamehash}{3daca00ebd2c6167a8aa9790b1d326a8}
      \strng{authorbibnamehash}{3daca00ebd2c6167a8aa9790b1d326a8}
