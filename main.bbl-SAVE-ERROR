        {{hash=4e233aa9aa21cf4bbc59a47976a0aa1d}{%
           family={Siart},
           familyi={S\bibinitperiod},
           given={Uwe},
           giveni={U\bibinitperiod}}}%
        {{hash=187f734d83455337adb74192c8e7cf01}{%
           family={Eibert},
           familyi={E\bibinitperiod},
