      \strng{namehash}{91b92e8a8ef8d2b3393750ae3eecdbee}
      \strng{fullhash}{16ed239e1af950da568c20c22e719ceb}
      \strng{bibnamehash}{91b92e8a8ef8d2b3393750ae3eecdbee}
