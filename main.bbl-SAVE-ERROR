    \entry{Nancel:2011}{inproceedings}{}
      \name{author}{5}{}{%
        {{hash=381e4495f0601024537a44d371cc5716}{%
           family={Nancel},
           familyi={N\bibinitperiod},
           given={Mathieu},
           giveni={M\bibinitperiod}}}%
        {{hash=9c50391dfae95d794c4ce6ab452ad4af}{%
           family={Wagner},
