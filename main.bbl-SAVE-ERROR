      \field{title}{Hand {Gesture} {Recognition} with {Ensemble} {Time}-{Frequency} {Signatures} {Using} {Enhanced} {Deep} {Convolutional} {Neural} {Network}}
      \field{year}{2019}
      \field{pages}{1602\bibrangedash 1605}
      \range{pages}{4}
