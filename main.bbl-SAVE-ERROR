      \strng{namehash}{0ec5824653f9635cbefcabe23d5de269}
      \strng{fullhash}{6a82db23d6af29ec28a60ed8c80bbf2d}
      \strng{bibnamehash}{0ec5824653f9635cbefcabe23d5de269}
