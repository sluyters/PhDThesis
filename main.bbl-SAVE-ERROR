      \strng{bibnamehash}{0b02682e7dbba8b0c04a8bf5db57286d}
      \strng{authorbibnamehash}{0b02682e7dbba8b0c04a8bf5db57286d}
      \strng{authornamehash}{0b02682e7dbba8b0c04a8bf5db57286d}
