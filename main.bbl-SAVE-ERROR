        {{hash=3b3b78fce1abb76e9d2eb9469b29c0e1}{%
           family={Yao},
           familyi={Y\bibinitperiod},
           given={Kai},
           giveni={K\bibinitperiod}}}%
        {{hash=10eaa900a144f0f63acd3deeab7a8e34}{%
           family={Chen},
