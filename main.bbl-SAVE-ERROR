        {{hash=4d0026a8acbc76f5eae94e86a0898801}{%
           family={Wang},
           familyi={W\bibinitperiod},
           given={Junxu},
           giveni={J\bibinitperiod}}}%
        {{hash=34667f9a9ac02f9b439df63c6fa33cba}{%
           family={Sheng},
