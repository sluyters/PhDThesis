        {{hash=f9b901055dfcef13d4fc6121a0435227}{%
           family={Evans},
           familyi={E\bibinitperiod},
           given={Robin\bibnamedelima J.},
           giveni={R\bibinitperiod\bibinitdelim J\bibinitperiod}}}%
        {{hash=78ca1136aa4546a230452fba153cbb95}{%
           family={Lech},
