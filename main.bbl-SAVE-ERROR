      \verb https://doi.org/10.1145/2909132.2909248
      \endverb
      \verb{url}
      \verb https://doi.org/10.1145/2909132.2909248
      \endverb
      \keyw{Browsing,Usability,Mid-Air Interaction,Selection,Gestures,Kinect,Elicitation,Image Collections}
