    \entry{Vogiatzidakis:2020}{article}{}
      \name{author}{2}{}{%
        {{hash=5c8fbdee829718fcc2eb3f267d8a01a6}{%
           family={Vogiatzidakis},
           familyi={V\bibinitperiod},
           given={Panagiotis},
           giveni={P\bibinitperiod}}}%
        {{hash=4e11216a6d3135760052f5a69e0b46df}{%
           family={Koutsabasis},
