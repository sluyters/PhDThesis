    \entry{Ritchie:2020}{article}{}
      \name{author}{3}{}{%
        {{hash=7a3871882b5ea1134b4fce7eb830a3d6}{%
           family={Ritchie},
           familyi={R\bibinitperiod},
           given={Matthew},
           giveni={M\bibinitperiod}}}%
        {{hash=8966dc82be6e614bf5f0eaa6607b5795}{%
           family={Capraru},
           familyi={C\bibinitperiod},
           given={Richard},
