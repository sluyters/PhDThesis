      \strng{namehash}{6403267011f5cf776c450e21c27e025b}
      \strng{fullhash}{ae65e0762b6f97f0f6f2c7009e1c40f5}
      \strng{bibnamehash}{6403267011f5cf776c450e21c27e025b}
